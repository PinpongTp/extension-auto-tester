{
  "manifest_version": 3,
  "name": "Auto Tester Extension",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/agreement-16.png",
    "32": "icons/agreement-32.png",
    "48": "icons/agreement-48.png",
    "128": "icons/agreement-128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "declarativeContent"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}